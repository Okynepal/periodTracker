{
  "expo": {
    "name": "Periodtracker",
    "slug": "periodtracker",
    "version": "3.0.2",
    "orientation": "default",
    "icon": "./src/resources/assets/app/icon.png",
    "splash": {
      "image": "./src/resources/assets/app/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "com.oky.nepal",
      "buildNumber": "3",
      "supportsTablet": true,
      "googleServicesFile": "./src/resources/GoogleService-Info.plist",
      "deploymentTarget": "12.0",
      "infoPlist": {
        "UIBackgroundModes": ["fetch", "remote-notification"]
      },
      "entitlements": {
        "aps-environment": "production"
      }
    },
    "android": {
      "package": "com.oky.nepal",
      "googleServicesFile": "./src/resources/google-services.json",
      "versionCode": 6,
      "supportsTablet": true,
      "permissions": ["INTERNET"],
      "compileSdkVersion": 36,
      "targetSdkVersion": 36
    },
    "web": {
      "favicon": "./src/resources/assets/app/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "127071a0-8561-4528-90e0-ff9782587903"
      }
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "useLegacyPackaging": true,
            "enableShrinkResourcesInReleaseBuilds": true,
            "enableProguardInReleaseBuilds": true,
            "compileSdkVersion": 36,
            "targetSdkVersion": 36,
            "manifest": {
              "application": {
                "meta-data": [
                  {
                    "android:name": "com.google.firebase.messaging.default_notification_channel_id",
                    "tools:replace": "android:value",
                    "android:value": "default"
                  },
                  {
                    "android:name": "com.google.firebase.messaging.default_notification_color",
                    "tools:replace": "android:resource",
                    "android:resource": "@color/notification_icon_color"
                  }
                ]
              }
            }
          }
        }
      ],
      "@react-native-firebase/app",
      "@react-native-firebase/crashlytics",
      "expo-localization",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ]
    ]
  }
}
